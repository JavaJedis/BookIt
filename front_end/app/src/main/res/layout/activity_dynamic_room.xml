<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DynamicRoomActivity">

    <ImageView
        android:id="@+id/room_imageView"
        android:layout_width="292dp"
        android:layout_height="230dp"
        android:fontFamily="@font/roboto_slab"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/room_name_textView"
        app:layout_constraintVertical_bias="0.016" />

    <TextView
        android:id="@+id/room_name_textView"
        android:layout_width="357dp"
        android:layout_height="61dp"
        android:fontFamily="@font/roboto_slab"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.296"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/address_textView"
        android:layout_width="385dp"
        android:layout_height="40dp"
        android:fontFamily="@font/roboto_slab"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/room_imageView"
        app:layout_constraintVertical_bias="0.023" />

    <TextView
        android:id="@+id/capacity_textView"
        android:layout_width="385dp"
        android:layout_height="40dp"
        android:fontFamily="@font/roboto_slab"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address_textView"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/description_textView"
        android:layout_width="385dp"
        android:layout_height="160dp"
        android:elegantTextHeight="false"
        android:fontFamily="@font/roboto_slab"
        android:includeFontPadding="false"
        android:text="TextView"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/capacity_textView"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/directions_button"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:background="@drawable/regular_button"
        android:fontFamily="@font/roboto_slab"
        android:text="@string/directions_button"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.963" />

    <Button
        android:id="@+id/view_comments_button"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:background="@drawable/regular_button"
        android:fontFamily="@font/roboto_slab"
        android:text="@string/view_comments_button"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.062"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.86" />

    <Button
        android:id="@+id/book_now_button"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:background="@drawable/regular_button"
        android:fontFamily="@font/roboto_slab"
        android:text="@string/book_now_button"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.805"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.963" />

    <Button
        android:id="@+id/report_button"
        android:layout_width="154dp"
        android:layout_height="51dp"
        android:background="@drawable/regular_button"
        android:fontFamily="@font/roboto_slab"
        android:text="@string/report_button"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.805"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.86" />

    <TextView
        android:id="@+id/hours_textView"
        android:layout_width="385dp"
        android:layout_height="245dp"
        android:elegantTextHeight="false"
        android:fontFamily="@font/roboto_slab"
        android:includeFontPadding="false"
        android:textAlignment="viewStart"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.615"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/capacity_textView"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>